<Window x:Class="CCTech.barcode"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CCTech"
        mc:Ignorable="d"
        Title="barcode" Height="450" Width="800">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="/car4456.jpeg"/>
        </Grid.Background>
        <Image x:Name="n" HorizontalAlignment="Left" Height="179" Margin="70,217,0,0" VerticalAlignment="Top" Width="168" Source="/barcode.jpeg" Visibility="Hidden"/>
        <TextBox x:Name="anstext" HorizontalAlignment="Center" Margin="0,156,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="94" Height="33" TextChanged="TextBox_TextChanged" Background="#FFFCECC0"/>
        <Button x:Name="x" Content="x" HorizontalAlignment="Left" Margin="749,10,0,0" VerticalAlignment="Top" Height="26" Width="41" Background="#FFE82A08" Click="x_Click"/>

    </Grid>
</Window>
