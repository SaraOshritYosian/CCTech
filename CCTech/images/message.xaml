<Window x:Class="CCTech.images.message"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CCTech.images"
        mc:Ignorable="d"
        Title="message" Height="450" Width="800">
    <Grid>
        <Image x:Name="scan" Margin="234,72,234,72" Source="/scanQR.png" Stretch="Fill" Visibility="Hidden"/>
        <Button Content="2" Margin="69,308,616,72" Click="Button_Click"/>
        <Button x:Name="number1" Content="1" Margin="571,302,127,72" Click="Button_Click_1"/>
        <TextBox x:Name="message1" TextWrapping="Wrap" Text="There's a load right now, you can wait or we will find you another charging station nearby. If you are willing to wait press 1, and if you want to find you another charging station nearby press 2 and scan the QR." Margin="290,42,260,247" TextChanged="TextBox_TextChanged"/>

    </Grid>
</Window>
