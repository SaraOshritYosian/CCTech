        "x = df[['RM']]\n",
        "y = df['Price']\n",
        "x = (x - x.mean())/x.std()\n",
        "#y = (y - y.mean())/y.std()\n",
        "plt.scatter(x,y)\n",
        "plt.xlabel('Number of rooms per house', size = 20)\n",
        "plt.ylabel('House Price', size = 20)\n",
        "plt.show()\n",
        "#x"
      ]
    },
